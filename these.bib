	keywords = {Biosynthetic Pathways, Computer simulation, Databases, Genetic, Homologous Recombination, Internet, Molecular Sequence Annotation, Multigene Family, Peptide Synthases, Polyketide Synthases, Recombinant Fusion Proteins}
}

