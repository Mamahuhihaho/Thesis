	urldate = {2016-01-11},
	date = {2012-09-29},
	langid = {english},
	keywords = {Animal Anatomy / Morphology / Histology, Computer Applications in Chemistry, drug discovery, Nonribosomal peptides, Physical Chemistry, Similarity searching, Target Prediction},
	file = {Full Text PDF:/home/dufresne/.zotero/zotero/viqdurf0.default/zotero/storage/XFFZXEAZ/Abdo et al. - 2012 - A new fingerprint to predict nonribosomal peptides.pdf:application/pdf;Snapshot:/home/dufresne/.zotero/zotero/viqdurf0.default/zotero/storage/EB9XPVMS/10.html:text/html}
}

