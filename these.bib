@article{orlin_line-digraphs_1978,
	title = {Line-digraphs, arborescences, and theorems of tutte and knuth},
	volume = {25},
	issn = {0095-8956},
	url = {http://www.sciencedirect.com/science/article/pii/0095895678900382},
	doi = {10.1016/0095-8956(78)90038-2},
	abstract = {The line-digraph of a digraph D with vertices V1, …, Vn is the digraph D∗ obtained from D by associating with each edge of D a vertex of D∗, and then directing an edge from vertex (Vi, Vj) of D∗ to vertex (Vk, Vm) if and only if j = k. This paper extends a characterization given by Harary and Norman for linedigraphs. It is also possible to repeatedly contract vertices of the line-digraph (with a new contraction procedure) so as to obtain the digraph derived from D by deleting all vertices with no incoming edges. Several new identities for arborescences are presented, leading to a combinatorial proof of Knuth's formula for the number of arborescences of a line-digraph. A new proof is given for the fact that in a digraph with every vertex having indegree equal to outdegree, the number of arborescences with root Vj is independent of j. Finally a new proof is presented for Tutte's Matrix Tree Theorem which shows the theorem to be a special case of the principle of inclusion-exclusion.},
	pages = {187--198},
	number = {2},
	journaltitle = {Journal of Combinatorial Theory, Series B},
	shortjournal = {Journal of Combinatorial Theory, Series B},
	author = {Orlin, James B},
	urldate = {2015-07-23},
	date = {1978-10},
	file = {ScienceDirect Full Text PDF:/home/dufresne/.zotero/zotero/viqdurf0.default/zotero/storage/UQQ88PSD/Orlin - 1978 - Line-digraphs, arborescences, and theorems of tutt.pdf:application/pdf;ScienceDirect Snapshot:/home/dufresne/.zotero/zotero/viqdurf0.default/zotero/storage/4F2GT5N4/0095895678900382.html:text/html}
}

