@article{novak_cyclobranch:_2015,
	title = {{CycloBranch}: De Novo Sequencing of Nonribosomal Peptides from Accurate Product Ion Mass Spectra},
	volume = {26},
	issn = {1044-0305, 1879-1123},
	url = {http://link.springer.com/article/10.1007/s13361-015-1211-1},
	doi = {10.1007/s13361-015-1211-1},
	shorttitle = {{CycloBranch}},
	abstract = {Nonribosomal peptides have a wide range of biological and medical applications. Their identification by tandem mass spectrometry remains a challenging task. A new open-source de novo peptide identification engine {CycloBranch} was developed and successfully applied in identification or detailed characterization of 11 linear, cyclic, branched, and branch-cyclic peptides. {CycloBranch} is based on annotated building block databases the size of which is defined by the user according to ribosomal or nonribosomal peptide origin. The current number of involved nonisobaric and isobaric building blocks is 287 and 521, respectively. Contrary to all other peptide sequencing tools utilizing either peptide libraries or peptide fragment libraries, {CycloBranch} represents a true de novo sequencing engine developed for accurate mass spectrometric data. It is a stand-alone and cross-platform application with a graphical and user-friendly interface; it supports {mzML}, {mzXML}, mgf, txt, and baf file formats and can be run in parallel on multiple threads. It can be downloaded for free from http://ms.biomed.cas.cz/cyclobranch/, where the User’s manual and video tutorials can be found. Graphical Abstract ᅟ},
	pages = {1780--1786},
	number = {10},
	journaltitle = {Journal of The American Society for Mass Spectrometry},
	shortjournal = {J. Am. Soc. Mass Spectrom.},
	author = {Novák, Jiří and Lemr, Karel and Schug, Kevin A. and Havlíček, Vladimír},
	urldate = {2016-08-04},
	date = {2015-07-21},
	langid = {english},
	file = {Full Text PDF:/home/dufresne/.zotero/zotero/viqdurf0.default/zotero/storage/S9E5WEM7/Novák et al. - 2015 - CycloBranch De Novo Sequencing of Nonribosomal Pe.pdf:application/pdf;Snapshot:/home/dufresne/.zotero/zotero/viqdurf0.default/zotero/storage/GDKP4MPH/s13361-015-1211-1.html:text/html}
